convection_x[i, j] = uFn*un + uFe*ue - uFs*us - uFw*uw / ?_x
convection_y[i, j] = vFn*vn + vFe*ve - vFs*vs - vFw*vw / ?_y

@E { Delta x = sx sub i - sx sub { i - 1 } }
@E { Delta x sup - = sx sub i - sx sub { i - 1 } }
@E { Delta x sup + = sx sub { i + 1 } - sx sub i }
@E { Delta y = sy sub j - sy sub { j - 1 } }
@E { Delta y sup - = sy sub i - sy sub { i - 1 } }
@E { Delta y sup + = sy sub { i + 1 } - sy sub i }
